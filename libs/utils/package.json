{
  "version": "0.0.6",
  "name": "@beenest/utils",
  "displayName": "Utils",
  "description": "Common utilities",
  "keywords": [
    "utils"
  ],
  "homepage": "https://beemood.github.io/beenest/libs/utils",
  "icon": "https://beemood.github.io/beenest/libs/utils/assets/favicon.png",
  "author": {
    "name": "Robert Brightline",
    "email": "robert-brightline@gmail.com",
    "url": "https://beemood.github.io/profile"
  },
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bugs": {
    "email": "robert-brightline@gmail.com"
  },
  "sponsor": {
    "url": "beemood.github.io"
  },
  "repository": {
    "directory": "libs/utils",
    "type": "github",
    "url": "https://github.com/beemood/beenest"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/utils/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/utils/dist",
          "main": "libs/utils/src/index.ts",
          "tsConfig": "libs/utils/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  }
}
