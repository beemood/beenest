{
  "version": "0.0.2",
  "name": "@beenest/prisma",
  "displayName": "Nestjs Prisma Module",
  "description": "Nestjs prisma module that provides prisma client and repositories",
  "keywords": [
    "Nestjs",
    "Prisma",
    "DI"
  ],
  "homepage": "https://beemood.github.io/beenest/libs/prisma",
  "icon": "https://beemood.github.io/beenest/libs/prisma/assets/favicon.png",
  "author": {
    "name": "Robert Brightline",
    "email": "robert-brightline@gmail.com",
    "url": "https://beemood.github.io/profile"
  },
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bugs": {
    "email": "robert-brightline@gmail.com"
  },
  "sponsor": {
    "url": "beemood.github.io"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/prisma/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/prisma/dist",
          "main": "libs/prisma/src/index.ts",
          "tsConfig": "libs/prisma/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@beenest/utils": "workspace:*",
    "@nestjs/common": "^11.1.6",
    "@swc/helpers": "~0.5.11",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6"
  }
}
